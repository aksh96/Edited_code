<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".gui.OpenMediumLevel">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="          MEDIUM LEVEL"
        android:textColor="#ed1991"
        android:layout_gravity="center"
        android:textSize="30sp"
        android:textAlignment="center"
        android:textStyle="normal|bold"
        android:layout_marginTop="22dp"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="430dp"
        android:layout_marginTop="5dp"
        >

        <!--first row-->

        <FrameLayout
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="45dp"
             android:id="@+id/framelayout1"
            >
            <ImageView
                android:layout_height="70dp"
                android:layout_width="70dp"
                android:src="@drawable/lockf5"
                android:layout_gravity="center"
                android:id="@+id/levelM_1_imageview"
                android:onClick="openMedium_1"
                >
            </ImageView>
        </FrameLayout>
        <FrameLayout
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_below="@+id/framelayout1"
            android:layout_alignEnd="@+id/framelayout1"
            android:layout_marginEnd="29dp"
            android:id="@+id/levelLabel_1">
            <TextView
                android:text="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/framelayout1"
                android:id="@+id/textView4"
                android:textStyle="normal|bold"
                android:textColor="#ed1991"
                android:layout_gravity="center"
                android:textSize="12sp" />
        </FrameLayout>


        <FrameLayout
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="28dp"
            android:layout_alignBottom="@+id/framelayout1"
            android:layout_toRightOf="@+id/framelayout1"
            android:layout_toEndOf="@+id/framelayout1"
            android:layout_centerHorizontal="true"
            android:id="@+id/framelayout2"
             >

            <ImageView
                android:layout_height="70dp"
                android:layout_width="70dp"
                android:src="@drawable/lockf5"
                android:layout_gravity="center"
                android:id="@+id/levelM_2_imageview"
                android:onClick="openMedium_2"
                >
            </ImageView>
        </FrameLayout>
        <FrameLayout
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_below="@+id/framelayout2"
            android:layout_marginLeft="183dp"
            android:id="@+id/Levellable_2">
            <TextView
                android:text="2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/framelayout1"
                android:id="@+id/textView3"
                android:textStyle="normal|bold"
                android:textColor="#ed1991"
                android:layout_gravity="center"
                android:textSize="12sp" />
        </FrameLayout>


        <FrameLayout
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="20dp"
            android:layout_alignBottom="@+id/framelayout2"
            android:layout_toRightOf="@+id/framelayout2"
            android:layout_toEndOf="@+id/framelayout2"
            android:id="@+id/framelayout3"
             >
            <ImageView
                android:layout_height="70dp"
                android:layout_width="70dp"
                android:src="@drawable/lockf5"
                android:layout_gravity="center"
                android:id="@+id/levelM_3_imageview"
                android:onClick="openMedium_3"
