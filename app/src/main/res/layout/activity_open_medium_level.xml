                android:textStyle="normal|bold"
                android:textColor="#ed1991"
                android:layout_gravity="center"
                android:textSize="12sp" />
        </FrameLayout>


        <FrameLayout
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="28dp"
            android:layout_alignBottom="@+id/framelayout4"
            android:layout_toRightOf="@+id/framelayout4"
            android:layout_toEndOf="@+id/framelayout4"
            android:id="@+id/framelayout5"
             >
            <ImageView
                android:layout_height="70dp"
                android:layout_width="70dp"
                android:src="@drawable/lockf5"
                android:layout_gravity="center"
                android:onClick="openMedium_5"
                android:id="@+id/levelM_5_imageview"
                >
            </ImageView>
        </FrameLayout>
        <FrameLayout
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_below="@+id/framelayout5"
            android:layout_marginLeft="183dp"

            android:id="@+id/Levellable_4">
            <TextView
                android:text="5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/framelayout5"
                android:id="@+id/textView7"
                android:textStyle="normal|bold"
                android:textColor="#ed1991"
                android:layout_gravity="center"
                android:textSize="12sp" />
        </FrameLayout>






